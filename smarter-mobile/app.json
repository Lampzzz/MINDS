{
  "expo": {
    "name": "Smarter",
    "slug": "smarter",
    "scheme": "Smarter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/smarter-circle-icon.png",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "newArchEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-432.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gdumy14.smarter"
    },
    "web": {
      "favicon": "./assets/images/smarter-circle-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-1024.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "imageWidth": 100
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/smarter-circle-icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c5a23e9b-8e18-4e8b-a31c-47c20cf04ea8"
      }
    }
  }
}
